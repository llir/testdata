!llvm.dbg.cu = !{!1}
!llvm.module.flags = !{!4}
!named = !{!0, !3}

!0 = distinct !DISubprogram(unit: !1)
!1 = distinct !DICompileUnit(language: DW_LANG_C99, file: !2, producer: "clang", isOptimized: true, flags: "-O2", splitDebugFilename: "abc.debug", emissionKind: LineTablesOnly)
!2 = !DIFile(filename: "path/to/file", directory: "/path/to/dir")
!3 = !DILocalVariable(name: "foo", arg: 65535, scope: !0)
!4 = !{i32 2, !"Debug Info Version", i32 3}
